{
  "name": "selfkey-wallet",
  "version": "1.0.0",
  "author": "Selfkey",
  "scripts": {
    "start": "react-native start",
    "storybook": "(adb reverse tcp:7007 tcp:7007 || true) && start-storybook",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "bundle:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "release:ios": "react-native run-ios --configuration Release",
    "bundle:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/IdentityWalletMobile/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "test": "jest",
    "lint": "eslint .",
    "lint:style": "stylelint './src/**/*.js'",
    "flow": "flow",
    "fix-deps": "sh fix-deps.sh && npx jetify && yarn rn-nodeify --hack",
    "postinstall": "npx patch-package",
    "postinstall2": "yarn fix-deps",
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "serve": "webpack serve",
    "start:web": "./node_modules/.bin/webpack-dev-server  --config ./web/webpack.config.js",
    "build:web": "./node_modules/.bin/webpack  --config ./web/webpack.config.js",
    "start:electron": "electron-forge start",
    "make": "electron-forge make",
    "package": "electron-forge package",
    "build:electron:mac": "./node_modules/.bin/electron-builder build  --mac",
    "build:electron:win": "./node_modules/.bin/electron-builder build  --win"
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/IdentityWalletMobile/Build/Products/Debug-iphonesimulator/IdentityWalletMobile.app",
        "build": "xcodebuild -project ios/IdentityWalletMobile.xcodeproj -scheme \"IdentityWalletMobile 2\"  -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build -UseModernBuildSystem=NO",
        "type": "ios.simulator",
        "device": {
          "type": "iPhone 11"
        }
      }
    }
  },
  "main": ".webpack/main",
  "build": {
    "appId": "com.selfkey.wallet",
    "productName": "selfkey",
    "mac": {
      "icon": "./dist/selfkey-icon.png"
    },
    "dmg": {},
    "mas": {},
    "win": {
      "icon": "./dist/selfkey-icon.png",
      "publisherName": "Selfkey"
    },
    "appx": {},
    "portable": {},
    "linux": {}
  },
  "files": [
    "dist/**/*",
    "electron/**/*",
    "!**/node_modules/*",
    "!**/dist/*",
    "!**/src/*"
  ],
